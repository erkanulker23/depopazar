# =============================================================================
# DepoPazar – PHP Uygulaması (Laravel Forge)
# Bu dosyayı .env olarak kopyalayıp Forge Environment'dan veya elle doldurun.
# Deploy sırasında php-app/config/db.local.php bu değerlerden oluşturulur.
# =============================================================================

# Uygulama
APP_NAME=DepoPazar
APP_ENV=production
APP_DEBUG=false
APP_URL=https://your-domain.com

# Veritabanı – Forge'da oluşturduğunuz MySQL veritabanı bilgileri
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=depotakip
DB_USERNAME=forge
DB_PASSWORD=
